{
    "name": "fpisani/kraber-container",
    "type": "library",
    "description": "",
    "keywords": [],
    "homepage": "https://github.com/f-pisani/kraber-container",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Pisani",
            "email": "pisani.florian@gmail.com",
